  <link name="knee_br_link">
    <inertial>
      <origin xyz="0 0 -0.1"/>
      <mass value="0.3"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.00002"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.1" rpy="0 0 0"/>
      <geometry><cylinder length="0.2" radius="0.03"/></geometry>
