<?xml version="1.0"?>
<robot name="4-spherical_4-revolute_spider-bot">
  
  <link name="body_core">
    <inertial>
      <origin xyz="0 0 0"/>
      <mass value="1.5"/>
      <inertia ixx="0.03" ixy="0" ixz="0" iyy="0.03" iyz="0" izz="0.03"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.25"/>
      </geometry>
      <material name="body_brown">
        <color rgba="0.5 0.3 0.1 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.25"/>
      </geometry>
    </collision>
  </link>

  <material name="leg_material">
    <color rgba="0.7 0.5 0.3 1"/>
  </material>

  <link name="hip_fl_link">
    <inertial>
      <origin xyz="0 0 -0.1"/>
      <mass value="0.3"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.00002"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.1" rpy="0 0 0"/>
      <geometry><cylinder length="0.2" radius="0.03"/></geometry>
      <material name="leg_material"/>
    </visual>
    <collision>
      <origin xyz="0 0 -0.1" rpy="0 0 0"/>
      <geometry><cylinder length="0.2" radius="0.03"/></geometry>
    </collision>
  </link>

  <joint name="hip_fl" type="spherical">
    <dynamics damping="2.0" friction="1.0"/>
    <origin xyz="0.2 0.2 0" rpy="0 0 0"/>
    <parent link="body_core"/>
    <child link="hip_fl_link"/>
    <limit effort="50" velocity="5.0"/>
  </joint>

  <link name="knee_fl_link">
    <inertial>
      <origin xyz="0 0 -0.1"/>
      <mass value="0.3"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.00002"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.1" rpy="0 0 0"/>
      <geometry><cylinder length="0.2" radius="0.03"/></geometry>
      <material name="leg_material"/>
    </visual>
    <collision>
      <origin xyz="0 0 -0.1" rpy="0 0 0"/>
      <geometry><cylinder length="0.2" radius="0.03"/></geometry>
    </collision>
  </link>

  <joint name="knee_fl" type="revolute">
    <dynamics damping="2.0" friction="1.0"/>
    <origin xyz="0.0 0.0 -0.2" rpy="0 0 0"/>
    <parent link="hip_fl_link"/>
    <child link="knee_fl_link"/>
    <axis xyz="1 0 0"/>
    <limit effort="40" velocity="5.0" lower="-0.1" upper="2.5"/>
  </joint>

  <link name="hip_fr_link">
    <inertial>
      <origin xyz="0 0 -0.1"/>
      <mass value="0.3"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.00002"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.1" rpy="0 0 0"/>
      <geometry><cylinder length="0.2" radius="0.03"/></geometry>
      <material name="leg_material"/>
    </visual>
    <collision>
      <origin xyz="0 0 -0.1" rpy="0 0 0"/>
      <geometry><cylinder length="0.2" radius="0.03"/></geometry>
    </collision>
  </link>

  <joint name="hip_fr" type="spherical">
    <dynamics damping="2.0" friction="1.0"/>
    <origin xyz="0.2 -0.2 0" rpy="0 0 0"/>
    <parent link="body_core"/>
    <child link="hip_fr_link"/>
    <limit effort="50" velocity="5.0"/>
  </joint>

  <link name="knee_fr_link">
    <inertial>
      <origin xyz="0 0 -0.1"/>
      <mass value="0.3"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.00002"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.1" rpy="0 0 0"/>
      <geometry><cylinder length="0.2" radius="0.03"/></geometry>
      <material name="leg_material"/>
    </visual>
    <collision>
      <origin xyz="0 0 -0.1" rpy="0 0 0"/>
      <geometry><cylinder length="0.2" radius="0.03"/></geometry>
    </collision>
  </link>

  <joint name="knee_fr" type="revolute">
    <dynamics damping="2.0" friction="1.0"/>
    <origin xyz="0.0 0.0 -0.2" rpy="0 0 0"/>
    <parent link="hip_fr_link"/>
    <child link="knee_fr_link"/>
    <axis xyz="1 0 0"/>
    <limit effort="40" velocity="5.0" lower="-0.1" upper="2.5"/>
  </joint>

  <link name="hip_bl_link">
    <inertial>
      <origin xyz="0 0 -0.1"/>
      <mass value="0.3"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.00002"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.1" rpy="0 0 0"/>
      <geometry><cylinder length="0.2" radius="0.03"/></geometry>
      <material name="leg_material"/>
    </visual>
    <collision>
      <origin xyz="0 0 -0.1" rpy="0 0 0"/>
      <geometry><cylinder length="0.2" radius="0.03"/></geometry>
    </collision>
  </link>

  <joint name="hip_bl" type="spherical">
    <dynamics damping="2.0" friction="1.0"/>
    <origin xyz="-0.2 0.2 0" rpy="0 0 0"/>
    <parent link="body_core"/>
    <child link="hip_bl_link"/>
    <limit effort="50" velocity="5.0"/>
  </joint>

  <link name="knee_bl_link">
    <inertial>
      <origin xyz="0 0 -0.1"/>
      <mass value="0.3"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.00002"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.1" rpy="0 0 0"/>
      <geometry><cylinder length="0.2" radius="0.03"/></geometry>
      <material name="leg_material"/>
    </visual>
    <collision>
      <origin xyz="0 0 -0.1" rpy="0 0 0"/>
      <geometry><cylinder length="0.2" radius="0.03"/></geometry>
    </collision>
  </link>

  <joint name="knee_bl" type="revolute">
    <dynamics damping="2.0" friction="1.0"/>
    <origin xyz="0.0 0.0 -0.2" rpy="0 0 0"/>
    <parent link="hip_bl_link"/>
    <child link="knee_bl_link"/>
    <axis xyz="1 0 0"/>
    <limit effort="40" velocity="5.0" lower="-0.1" upper="2.5"/>
  </joint>

  <link name="hip_br_link">
    <inertial>
      <origin xyz="0 0 -0.1"/>
      <mass value="0.3"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.00002"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.1" rpy="0 0 0"/>
      <geometry><cylinder length="0.2" radius="0.03"/></geometry>
      <material name="leg_material"/>
    </visual>
    <collision>
      <origin xyz="0 0 -0.1" rpy="0 0 0"/>
      <geometry><cylinder length="0.2" radius="0.03"/></geometry>
    </collision>
  </link>

  <joint name="hip_br" type="spherical">
