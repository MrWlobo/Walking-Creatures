      <material name="leg_material"/>
    </visual>
    <collision>
      <origin xyz="0 0 -0.1" rpy="0 0 0"/>
      <geometry><capsule length="0.18" radius="0.03"/></geometry>
    </collision>
    <collision>
        <origin xyz="0 0 -0.2" rpy="0 0 0"/>
        <geometry><sphere radius="0.03"/></geometry>
    </collision>
  </link>

  <joint name="knee_fl" type="revolute">
    <dynamics damping="1.0" friction="0.5"/>
    <origin xyz="0.0 0.0 -0.2" rpy="0 0 0"/>
    <parent link="hip_fl_link"/>
    <child link="knee_fl_link"/>
    <axis xyz="1 0 0"/>
    <limit effort="30" velocity="8.0" lower="-0.1" upper="2.5"/>
  </joint>

