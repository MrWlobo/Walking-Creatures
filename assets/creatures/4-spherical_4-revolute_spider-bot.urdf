<?xml version="1.0"?>
<robot name="4-spherical_4-revolute_spider-bot">
  <link name="body_core">
    <inertial><origin xyz="0 0 0"/><mass value="3"/><inertia ixx="0.03" ixy="0" ixz="0" iyy="0.03" iyz="0" izz="0.03"/></inertial>
    <visual><origin xyz="0 0 0"/><geometry><sphere radius="0.25"/></geometry><material name="body_brown"><color rgba="0.5 0.3 0.1 1"/></material></visual>
    <collision><origin xyz="0 0 0"/><geometry><sphere radius="0.25"/></geometry></collision>
  </link>

  <!-- Material for the legs -->
  <material name="leg_material"><color rgba="0.7 0.5 0.3 1"/></material>

  <!-- ********************* -->
  <!-- *** FRONT LEFT LEG *** -->
  <!-- ********************* -->
  <link name="hip_fl_link">
    <inertial>
      <origin xyz="0 0 -0.1"/><mass value="0.1"/><inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.00002"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.1" rpy="0 0 0"/>
      <geometry><cylinder length="0.2" radius="0.03"/></geometry>
      <material name="leg_material"/>
    </visual>
    <collision>
      <origin xyz="0 0 -0.1" rpy="0 0 0"/>
      <geometry><cylinder length="0.2" radius="0.03"/></geometry>
    </collision>
  </link>
  <joint name="hip_fl" type="spherical">
    <origin xyz="0.2 0.2 0" rpy="0 0 0"/>
    <parent link="body_core"/><child link="hip_fl_link"/>
    <limit lower="-1.0 -1.0 -1.0" upper="1.0 1.0 1.0" effort="10" velocity="5"/>
  </joint>
  
  <link name="knee_fl_link">
    <inertial>
      <origin xyz="0 0 -0.1"/><mass value="0.1"/><inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.00002"/>
