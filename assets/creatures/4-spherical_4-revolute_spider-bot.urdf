<?xml version="1.0"?>
<robot name="4-spherical_4-revolute_spider-bot_heavy">
  
  <link name="body_core">
    <inertial>
      <origin xyz="0 0 0"/>
      <mass value="15.0"/>
      <inertia ixx="0.4" ixy="0" ixz="0" iyy="0.4" iyz="0" izz="0.4"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0"/>
      <geometry><sphere radius="0.25"/></geometry>
      <material name="body_brown">
        <color rgba="0.5 0.3 0.1 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0"/>
      <geometry><sphere radius="0.25"/></geometry>
    </collision>
  </link>

  <material name="leg_material">
    <color rgba="0.7 0.5 0.3 1"/>
  </material>

  <link name="hip_fl_link">
    <inertial>
      <origin xyz="0 0 -0.1"/>
      <mass value="3.0"/>
      <inertia ixx="0.05" ixy="0" ixz="0" iyy="0.05" iyz="0" izz="0.01"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.1" rpy="0 0 0"/>
      <geometry><cylinder length="0.2" radius="0.03"/></geometry>
      <material name="leg_material"/>
    </visual>
    <collision>
      <origin xyz="0 0 -0.1" rpy="0 0 0"/>
      <geometry><capsule length="0.18" radius="0.02"/></geometry> 
    </collision>
  </link>

  <joint name="hip_fl" type="spherical">
    <dynamics damping="2.0" friction="0.8"/>
    <origin xyz="0.2 0.2 0" rpy="0 0 0"/>
    <parent link="body_core"/>
    <child link="hip_fl_link"/>
    <axis xyz="0 0 0"/> 
    <limit effort="45" velocity="6.0" lower="-0.5" upper="0.5"/>
  </joint>

  <link name="knee_fl_link">
    <contact>
      <lateral_friction value="1.0"/>
      <spinning_friction value="0.1"/>
    </contact>
    <inertial>
      <origin xyz="0 0 -0.1"/>
      <mass value="3.0"/>
      <inertia ixx="0.05" ixy="0" ixz="0" iyy="0.05" iyz="0" izz="0.01"/>
