    <collision>
      <origin xyz="0 0 -0.1" rpy="0 0 0"/>
      <geometry><cylinder length="0.2" radius="0.03"/></geometry>
    </collision>
  </link>
  <joint name="knee_fl" type="revolute">
    <origin xyz="0.0 0.0 -0.2" rpy="0 0 0"/>
    <parent link="hip_fl_link"/><child link="knee_fl_link"/>
    <axis xyz="1 0 0"/><limit lower="-1.57" upper="0" effort="8" velocity="4"/>
  </joint>

  <!-- ********************* -->
  <!-- *** FRONT RIGHT LEG *** -->
