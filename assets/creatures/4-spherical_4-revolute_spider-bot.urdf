  </link>

  <joint name="knee_br" type="revolute">
    <dynamics damping="2.0" friction="1.0"/>
    <origin xyz="0.0 0.0 -0.2" rpy="0 0 0"/>
    <parent link="hip_br_link"/>
    <child link="knee_br_link"/>
    <axis xyz="1 0 0"/>
    <limit effort="40" velocity="5.0" lower="-0.1" upper="2.5"/>
  </joint>

</robot>