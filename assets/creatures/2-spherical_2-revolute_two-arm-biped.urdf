<?xml version="1.0"?>
<robot name="2-spherical_2-revolute_two-arm-biped">
  <link name="torso">
    <inertial><origin xyz="0 0 0"/><mass value="2"/><inertia ixx="0.02" ixy="0" ixz="0" iyy="0.02" iyz="0" izz="0.02"/></inertial>
    <visual><origin xyz="0 0 0"/><geometry><sphere radius="0.18"/></geometry><material name="torso_gray"><color rgba="0.6 0.6 0.6 1"/></material></visual>
    <collision><origin xyz="0 0 0"/><geometry><sphere radius="0.18"/></geometry></collision>
  </link>

  <link name="left_leg">
    <inertial><origin xyz="0 0 -0.1"/><mass value="0.4"/><inertia ixx="0.0005" ixy="0" ixz="0" iyy="0.0005" iyz="0" izz="0.0005"/></inertial>
    <visual><origin xyz="0 0 -0.15"/><geometry><sphere radius="0.08"/></geometry><material name="leg_silver"><color rgba="0.8 0.8 0.8 1"/></material></visual>
    <collision><origin xyz="0 0 -0.15"/><geometry><sphere radius="0.08"/></geometry></collision>
  </link>
  <joint name="hip_left" type="revolute">
    <origin xyz="0 0.1 0" rpy="0 0 0"/>
    <parent link="torso"/><child link="left_leg"/>
    <axis xyz="1 0 0"/><limit lower="-0.785" upper="0.785" effort="12" velocity="6"/>
  </joint>

  <link name="right_leg">
    <inertial><origin xyz="0 0 -0.1"/><mass value="0.4"/><inertia ixx="0.0005" ixy="0" ixz="0" iyy="0.0005" iyz="0" izz="0.0005"/></inertial>
    <visual><origin xyz="0 0 -0.15"/><geometry><sphere radius="0.08"/></geometry><material name="leg_silver"/></visual>
    <collision><origin xyz="0 0 -0.15"/><geometry><sphere radius="0.08"/></geometry></collision>
  </link>
  <joint name="hip_right" type="revolute">
    <origin xyz="0 -0.1 0" rpy="0 0 0"/>
    <parent link="torso"/><child link="right_leg"/>
    <axis xyz="1 0 0"/><limit lower="-0.785" upper="0.785" effort="12" velocity="6"/>
  </joint>

  <link name="left_arm">
    <inertial><origin xyz="0 0 0"/><mass value="0.2"/><inertia ixx="0.0002" ixy="0" ixz="0" iyy="0.0002" iyz="0" izz="0.0002"/></inertial>
    <visual><origin xyz="0 0 0"/><geometry><sphere radius="0.07"/></geometry><material name="arm_gold"><color rgba="0.8 0.6 0.2 1"/></material></visual>
    <collision><origin xyz="0 0 0"/><geometry><sphere radius="0.07"/></geometry></collision>
  </link>
  <joint name="shoulder_left" type="spherical">
    <origin xyz="0.1 0.25 0.1" rpy="0 0 0"/>
    <parent link="torso"/><child link="left_arm"/>
    <limit lower="-1.57 -1.57 -1.57" upper="1.57 1.57 1.57" effort="10" velocity="5"/>
  </joint>

  <link name="right_arm">
    <inertial><origin xyz="0 0 0"/><mass value="0.2"/><inertia ixx="0.0002" ixy="0" ixz="0" iyy="0.0002" iyz="0" izz="0.0002"/></inertial>
    <visual><origin xyz="0 0 0"/><geometry><sphere radius="0.07"/></geometry><material name="arm_gold"/></visual>
    <collision><origin xyz="0 0 0"/><geometry><sphere radius="0.07"/></geometry></collision>
  </link>
  <joint name="shoulder_right" type="spherical">
    <origin xyz="0.1 -0.25 0.1" rpy="0 0 0"/>
    <parent link="torso"/><child link="right_arm"/>
    <limit lower="-1.57 -1.57 -1.57" upper="1.57 1.57 1.57" effort="10" velocity="5"/>
  </joint>
</robot>