<?xml version="1.0"?>
<robot name="4-spherical_quadruped">

  <link name="body">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="15.0"/>
      <inertia ixx="0.4" ixy="0" ixz="0" iyy="0.4" iyz="0" izz="0.4"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.22"/>
      </geometry>
      <material name="body_orange">
        <color rgba="1 0.5 0 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.22"/>
      </geometry>
    </collision>
  </link>

  <material name="leg_yellow">
    <color rgba="1 1 0 1"/>
  </material>

  <link name="leg_fl">
    <inertial>
      <origin xyz="0 0 -0.15" rpy="0 0 0"/>
      <mass value="5.0"/>
      <inertia ixx="0.05" ixy="0" ixz="0" iyy="0.05" iyz="0" izz="0.01"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.12" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.24" radius="0.02"/>
      </geometry>
      <material name="leg_yellow"/>
    </visual>
    <visual>
      <origin xyz="0 0 -0.24" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.02" radius="0.1"/>
      </geometry>
      <material name="leg_yellow"/>
    </visual>
    
    <collision>
      <origin xyz="0 0 -0.12" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.24" radius="0.02"/>
      </geometry>
    </collision>
    <collision>
      <origin xyz="0 0 -0.24" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.02" radius="0.1"/>
      </geometry>
    </collision>
  </link>
  
  <joint name="hip_fl" type="spherical">
    <origin xyz="0.2 0.2 0" rpy="0 0 0"/>
    <parent link="body"/>
    <child link="leg_fl"/>
    <axis xyz="0 0 0"/> 
    <limit effort="60" velocity="6.0" lower="-0.6" upper="0.6"/>
    <dynamics damping="2.0" friction="0.8"/>
  </joint>

  <link name="leg_fr">
